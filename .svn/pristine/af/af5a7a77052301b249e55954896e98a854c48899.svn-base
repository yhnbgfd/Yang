        private void SpecialStar(int[] arr, int ctab)
        {
            if (m_SpecialStar.ContainsKey(arr))
            {
                m_SpecialStar[arr] += ctab;
                if (m_SpecialStar[arr] == 0)
                    m_SpecialStar.Remove(arr);
            }
            else
            {
                if(ctab > 0)
                    m_SpecialStar.Add(arr, ctab);
            }
        }
        
