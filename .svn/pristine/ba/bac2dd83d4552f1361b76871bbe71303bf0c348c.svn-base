        /* 中奖号码的数组比较方法 */
        private void Winning_Numbers_Compare(int[] TValue, int cTab)
        {
            bool isSame = true;
            for (int i = 0; i < SelectNum; i++)
            {
                if (TValue[i] != Winning_Numbers[i])
                    isSame = false;
            }
            if (isSame)
            {
                Winning_Numbers_Mark += cTab;
                label34.Text = "中奖号码当前标记：" + Winning_Numbers_Mark.ToString();
            }
        }

