        private void button8_Click(object sender, EventArgs e)
        {
            //database color mark
            bool newColor = true;
            foreach (KeyValuePair<int, totalDataBase> kvp in m_totalDataBase)
            {
                kvp.Value.DiffColorNum = 0;
                for (int i = 0; i < SelectNum; i++)
                {
